--Load
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hakaiofc/Base/main/HUBORION')))()

--Main Script
local Window = OrionLib:MakeWindow({Name = "em Teste ü•ã V1", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})

--Valor
_G.AutoFlexao = True

--Fun√ß√£o
function AutoFlexao()
while _G.AutoFlexao == true do
game:GetService("ReplicatedStorage").Remotes.Pushup:FireServer()
wait(0.8)
end
end


--Valor
_G.AutoSitup = True

--Fun√ß√£o
function AutoSitup()
while _G.AutoSitup == true do
game:GetService("ReplicatedStorage").Remotes.Situp:FireServer()
wait(0.8)
end
end





--Main
local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = MainTab:AddSection({
    Name = "Training"
})
MainTab:AddToggle({
    Name = "Pushup",
    Default = false,
    Callback = function(Value)
        _G.AutoFlexao = Value
        AutoFlexao()
    end
})
MainTab:AddToggle({
    Name = "Situp",
    Default = false,
    Callback = function(Value)
        _G.AutoSitup = Value
        AutoSitup()
    end
})




--Dojo 
